{
  "name": "ldstats",
  "version": "2.5.0",
  "description": "Ratings API for Ludum Dare",
  "author": "Pablo Novas <pjnovas@gmail.com>",
  "license": "BSD-2-Clause",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:watch": "npm test -- --watch"
  },
  "betterScripts": {
    "dev": {
      "command": "node ./bin/www",
      "env": {
        "NODE_PATH": ".",
        "DEBUG": "ldstats:*"
      }
    },
    "test": {
      "command": "mocha --require babel-core/register --recursive --reporter spec --bail",
      "env": {
        "NODE_PATH": "."
      }
    }
  },
  "dependencies": {
    "async": "^1.4.2",
    "asynch": "^0.5.2",
    "axios": "^0.19.2",
    "babel-core": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.19.0",
    "cheerio": "^0.22.0",
    "cors": "^2.7.1",
    "debug": "~2.6.9",
    "express": "^4.17.1",
    "express-handlebars": "^2.0.1",
    "hbs": "~3.1.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.5",
    "morgan": "^1.10.0",
    "natives": "^1.1.6",
    "serve-favicon": "^2.5.0",
    "superagent": "^3.7.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-runtime": "^6.23.0",
    "better-npm-run": "0.0.15",
    "eslint": "^3.19.0",
    "mocha": "^3.4.2",
    "testdouble": "^3.0.0",
    "unexpected": "^10.29.0"
  }
}
